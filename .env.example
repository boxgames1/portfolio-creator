# Supabase (client-safe, exposed in browser)
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=

# Google OAuth (client-safe, add in Supabase Dashboard > Auth > Providers)
VITE_GOOGLE_CLIENT_ID=

# Optional: Direct DB URL for migrations
DB_URL=

# Server-only: Add these as Supabase Edge Function secrets (supabase secrets set)
# ALPHA_VANTAGE_API_KEY=  # Free at alphavantage.co - primary for stocks/ETFs (global symbols)
# TIINGO_API_KEY=      # Free at tiingo.com - fallback for stock/ETF prices
# FINNHUB_API_KEY=     # Free at finnhub.io - fallback (US stocks only on free tier)
# OPENAI_API_KEY=      # For AI suggestions, real estate estimation, portfolio chat, sentiment

# Stripe (for AI token purchases). Set in Supabase Dashboard > Edge Functions > Secrets
# STRIPE_SECRET_KEY=       # Stripe secret key (sk_live_... or sk_test_...)
# STRIPE_WEBHOOK_SECRET=   # From Stripe Dashboard > Developers > Webhooks (add endpoint for checkout.session.completed)
# APP_URL=                 # Public app URL for checkout redirects (e.g. https://yourapp.com or http://localhost:5173)
